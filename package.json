  "dependencies": {
    "argparse": "^0.1.15",
    "glob": "^4.0.0"
  },
  "devDependencies": {
    "coffeeify": "^0.7.0",
    "benchmark": "^1.0.0",
    "jasmine-node": "^1.14.5"
  },
  "bin": {
    "yaml2json": "./bin/yaml2json",
    "json2yaml": "./bin/json2yaml"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jeremyfa/yaml.js.git"
